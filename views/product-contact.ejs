            <!-- Contact Form -->
            <div class="contactform-wrap">
               <form action="#" class="form-validate">
                  <h3 class="component-ttl component-ttl-ct component-ttl-hasdesc"><span>Order</span></h3>
                  <p class="component-desc component-desc-ct">Please provide us with the following information</p>
                  <p class="contactform-field contactform-text">
                     <label class="contactform-label">Name*</label><!-- NO SPACE --><span class="contactform-input"><input placeholder="Name" type="text" name="name" data-required="text"></span>
                  </p>
                  <p class="contactform-field contactform-email">
                     <label class="contactform-label">E-mail*</label><!-- NO SPACE --><span class="contactform-input"><input placeholder="Your E-mail" type="email" name="email" data-required="email"></span>
                  </p>
                  <p class="contactform-field contactform-text">
                     <label class="contactform-label">Mobile Phone*</label><!-- NO SPACE --><span class="contactform-input"><input placeholder="Mobile Phone" type="text" name="phone" data-required="text"></span>
                  </p>
                  <p class="contactform-field contactform-text">
                     <label class="contactform-label">Quantity*</label><!-- NO SPACE --><span class="contactform-input"><input placeholder="Quantity* (Minimum of 10)" type="text" name="quantity" data-required="text"></span>
                  </p>
                  <p class="contactform-field contactform-text">
                    <%# <label class="contactform-label">Product Type*</label>%>
                    <span class="contactform-input">
                      <select style="margin-left: 120px; width: 80%;">
                        <option>--- Product Type* ---</option>
                        <option value="shirts">shirts</option>
                        <option value="hats">hats</option>
                        <option value="bags">bags</option>
                        <option value="socks">socks</option>
                        <option value="calendar">calendar</option>
                        <option value="mugs">mugs</option>
                        <option value="pillows">pillows</option>
                        <option value="phone-case">phone-case</option>
                      </select>
                    </span>
                  </p>
                  <p class="contactform-field contactform-textarea">
                     <label class="contactform-label">Imprint Information</label><!-- NO SPACE --><span class="contactform-input"><textarea placeholder="Text Imprint Color(s) - Text Imprint Size, Font, and Other Info - Logo Imprint Description - Logo Imprint Color(s)" name="message" data-required="text"></textarea></span>
                  </p>
                  <p class="contactform-field contactform-text">
                     <label class="contactform-label">Attach Logo Art File</label><!-- NO SPACE --><span class="contactform-input"><input placeholder="Attach Logo Art File" type="file" name="logo" multiple></span>
                  </p>
                  <p class="contactform-submit">
                     <input value="Send" type="submit">
                  </p>
               </form>
            </div>
         </section>
