<!doctype html>
<html class="no-js"  lang="en">
<head> 

    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, height=device-height, target-densitydpi=device-dpi, initial-scale=1.0, minimum-scale=1, maximum-scale=1.0, user-scalable=no" />
    <meta name="apple-mobile-web-app-capable" content="no">
    <meta name="format-detection" content="email=no">
    <meta name="HandheldFriendly" content="true">
    <title>SAGE Virtual Design</title>
    <meta name="description" content="SAGE Virtual Design a virtual design studio">
    <meta name="title" content="SAGE Virtual Design">
    <meta name="keywords" content="design, tool, virtual, sage">
    
    <link rel="stylesheet" href="css/styles.min.css" type="text/css">

</head>
<body style="overflow-y: auto !important">
<div class="wrapper">
    <div class="rightContent">
        <div class="toolbar">
            <ul>
                <li class="mobile-close mobile-only">+</li>
                <li class="mobile-only title">Tools</li>
                <li class="vd-option zoomIn noActiveObjectReq ed tooltip" title="Zoom In"><a href="#"><span>Zoom In</span></a></li>
                <li class="vd-option zoomOut noActiveObjectReq ed tooltip" title="Zoom Out"><a href="#"><span>Zoom Out</span></a></li>
                <li class="divider"></li>
                <li class="vd-option undo noActiveObjectReq ed tooltip" title="Undo"><a href="#"><span>Undo</span></a></li>
                <li class="vd-option redo noActiveObjectReq ed tooltip" title="Redo"><a href="#"><span>Redo</span></a></li>
                <li class="divider"></li>
                <li class="vd-option resizeRotate needsActiveObject imageOrText ed tooltip" title="Resize/Rotate"><a href="#"><span>Resize/Rotate</span></a></li>
                <li class="vd-option skew needsActiveObject imageOrText ed tooltip" title="Skew"><a href="#"><span>Skew</span></a></li>
                <li class="vd-option curve needsActiveObject imageOrText ed tooltip" title="Curve"><a href="#"><span>Curve</span></a></li>
                <li class="vd-option imageEdit noActiveObjectReq ed tooltip" title="Edit product image"><a href="#"><span>Edit product image</span></a></li>
                <li class="divider"></li>
                <li class="vd-option lastUsed needsActiveObject imageOrText ed tooltip" title="Use Last" style="display:none;"><a href="#"><span>Use Last</span></a></li>
                <li class="vd-option moveToBack needsActiveObject imageOrText ed tooltip" title="Move to Back"><a href="#"><span>Move to Back</span></a></li>
                <li class="vd-option bringToFront needsActiveObject imageOrText ed tooltip" title="Bring to Front"><a href="#"><span>Bring to Front</span></a></li>
                <li class="divider"></li>
                <li class="vd-option showImprint noActiveObjectReq ed tooltip" title="Show imprint area" style="display: none;"><a href="#"><span>Show imprint area</span></a></li>
                <li class="divider" style="display: none;"></li>
                <li class="vd-option delete needsActiveObject imageOrText ed tooltip" title="Delete"><a href="#"><span>Delete</span></a></li>
            </ul>
        </div>
        <div class="canvasScroll">
            <div class="canvasWrapper">
                <canvas id="myCanvas"></canvas>
            </div>
        </div>
        <div id="backgroundPicker" class="sageCarousel"></div>
        <div id="backgroundPickerToggle"><span></span><i></i></div>
        <div id="nonintrusive-loader" class="shadow"><i></i><span></span></div>
    </div>
    <button id="toggle-toolbar" class="mobile-only"></button>
    <button id="toggle-actions" class="mobile-only"></button>
    <button id="toggle-options" class="mobile-only disabled"></button>
    <button id="toggle-backgrounds" class="mobile-only force-state" style="display:none;"></button>
    <div id="companyBg"></div>
    <div id="legal" class="legal">Terms of Use</div>
    <div class="plotMode clearPlotMode">Clear</div>
    <div class="plotMode saveLogoLocation">Save Logo Location</div>
</div>

<div id="fontsPreloader" style="color: red;height: 0px;margin:0;padding:0"></div>

<div class="plot-cont">
  <div class="close-open">
    <img src="../images/icons/info.svg" alt="">
  </div>
  <div class="body">
    <h1>CURVE INFO</h1>
    <div class="line"></div>
    <div class="curve-info">
      <div>
        <p class="sub-titles">Inflection point:</p>
        <div class="influection cords">X: 0 , Y: 0</div>
      </div>
      <div>
        <p class="sub-titles">Curve point:</p>
        <div class="curvePoints cords">X: 0 , Y: 0</div>
      </div>
      <input type="text" class="curve-input">
    </div>
    <div class="line"></div>
    <h1>POSITION INFO</h1>
    <div class="line"></div>
    <div class="skew-info">
      <div>
        <p class="sub-titles">Top left:</p>
        <div class="top-left cords">X: 0 , Y: 0</div>
      </div>
      <div>
        <p class="sub-titles">Top right:</p>
        <div class="top-right cords">X: 0 , Y: 0</div>
      </div>
      <div>
        <p class="sub-titles">Bottom left:</p>
        <div class="bottom-left cords">X: 0 , Y: 0</div>
      </div>
      <div>
        <p class="sub-titles">Bottom right:</p>
        <div class="bottom-right cords">X: 0 , Y: 0</div>
      </div>
      <input type="text" class="skew-input">
    </div>
    <div class="line"></div>
    <div class="save-btn">
      <!--<img src="../images/icons/hosting.svg" alt="">
      <span>SAVE IMAGE</span>-->
    </div>
  </div>
</div>
<div class="func-buttons mobile-only force-state">
    <button class="button cancel" data-action="cancel">Cancel</button>
    <button class="button apply" data-action="apply">Apply</button>
    <div class="text shadow"></div>
</div>

<div id="ImageEdit" class="modal-overlay dark" style="display: none;">
    <div class="window panelContainer">
        <div class="title handler">
            <h3>Image Edit</h3>
            <div class="actions">
                <button class="button save">Done</button><button class="button discard">Cancel</button>
            </div>
        </div>
        <div class="title toolbar">
            <ul class="tools">
                <li class="crop tooltip" title="Crop"><a href="#"></a></li>
                <li class="paint tooltip" title="Paint"><a href="#"></a></li>
                <li class="fill tooltip" title="Fill"><a href="#"></a></li>
                <li class="rotate tooltip" title="Rotate clockwise"><a href="#"></a></li>
            </ul>
        </div>
        <div id="ImageEdit-tools" class="title toolbar">
            <ul class="default">
                <li>Choose one of the tools</li>
            </ul>
            <ul class="crop" style="display: none;">
                <li class="small"><button class="button crop-save">Apply</button></li>
                <li class="small"><button class="button crop-discard">Cancel</button></li>
            </ul>
            <ul class="paint" style="display: none;">
                <li class="undo tooltip" title="Undo"><a href="#"></a></li>
                <li class="redo tooltip" title="Redo"><a href="#"></a></li>
                <li>
                    <label>Color:</label>
                    <input type="hidden" id="brushColor" />
                </li>
                <li>
                    <label>Size:</label>
                    <select id="brushSize">
                        <option value="1">1</option>
                        <option value="3">3</option>
                        <option value="5">5</option>
                        <option selected value="10">10</option>
                        <option value="15">15</option>
                        <option value="20">20</option>
                        <option value="30">30</option>
                        <option value="50">50</option>
                    </select>
                </li>
            </ul>
            <ul class="fill" style="display: none;">
                <li class="undo tooltip" title="Undo"><a href="#"></a></li>
                <li class="redo tooltip" title="Redo"><a href="#"></a></li>
                <li>
                    <label>Color:</label>
                    <input type="hidden" id="fillColor" />
                </li>
                <li>
                    <label>Tolerance:</label>
                    <div id="toleranceSlider"></div>
                </li>
            </ul>
        </div>
        <div class="content">
            <canvas id="imageEditCanvas"></canvas>
        </div>
    </div>
</div>
<!-- <script src="js/scripts.min.js"></script> -->
</body>
</html>
